{
  "private": true,
  "name": "anyong",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "serve": "node server --title=anyong",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "post": "cpx ./public/maps/fengmap.min.js ./node_modules/fengmap/build --clean"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@umijs/preset-react": "1.x",
    "cpx": "^1.5.0",
    "dayjs": "^1.10.5",
    "echarts": "^5.1.2",
    "echarts-for-react": "^3.0.1",
    "fast-deep-equal": "^3.1.3",
    "fengmap": "^3.0.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.1",
    "react-fengmap": "^2.9.5",
    "umi": "^3.4.25"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.25",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
